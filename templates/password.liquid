{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}
{% layout none %}
<!DOCTYPE html>
<html>

<head>

  <title><ly-as-4182873>Will be opening soon...</ly-as-4182873></title>
  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js' | script_tag }}
  {% comment %}ly_asset_replace_for_[ 'jquery.fancybox.pack.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'jquery.fancybox.pack.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'jquery.mousewheel.pack.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'jquery.mousewheel.pack.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'shop.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'shop.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'style.css' | asset_url | stylesheet_tag ]_begin{% endcomment %}{% assign ly_asset = 'style.css' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | stylesheet_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'styleSDC.css' | asset_url | stylesheet_tag ]_begin{% endcomment %}{% assign ly_asset = 'styleSDC.css' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | stylesheet_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  
</head>
<body> 
  <div id='passwordHome'>
  <span class='laun'><ly-as-4285803>Launching May 2017</ly-as-4285803></span>
  <span class='weare'><ly-as-4285804>We're working on creating something fabulous!</ly-as-4285804></span>
  
  <div>
    <div class='admin'>
      <div>
        <!--<h1>{{ langify_shop_name }}</h1>-->
        {% if shop.password_message and shop.password_message != '' %}
          {{ shop.password_message }}
        {% else %}
        <p></p>
        {% endif %}
      </div>
      <!-- Entrada de adminsitrador-->
      {% form 'storefront_password' %}
        {{ form.errors | default_errors }}
        <label for='password'><ly-as-4182874>Enter store using password:</ly-as-4182874> </label>
        <input type='password' style='background-color=#45B39D;' name='password' id='password'/>
        <div class="actions">
          <input class="btn"  name="commit" type="submit" value="<ly-as-4285807>Admin</ly-as-4285807>" />
        </div>
      {% endform %}

    </div>
    <!-- Slideshow -->
      <div id="escritorio">
        <ul class="sdc-slideshow">
            <li><span><ly-as-4314606>Image 01</ly-as-4314606></span></li>
            <li><span><ly-as-4314607>Image 02</ly-as-4314607></span></li>
            <li><span><ly-as-4314608>Image 03</ly-as-4314608></span></li>
            <li><span><ly-as-4314609>Image 04</ly-as-4314609></span></li>
            
        </ul>
        
      </div>
      <div id="movil">
          <ul class="sdc-slideshow2">
              <li><span><ly-as-4314606>Image 01</ly-as-4314606></span></li>
              <li><span><ly-as-4314607>Image 02</ly-as-4314607></span></li>
              <li><span><ly-as-4314608>Image 03</ly-as-4314608></span></li>
              <li><span><ly-as-4314609>Image 04</ly-as-4314609></span></li>

          </ul>

      </div>
    <div class='headerCenter logoSolo'>
      <span><img src="//cdn.shopify.com/s/files/1/1736/7383/t/16/assets/logo.svg" alt="<ly-as-4314610>logo-yshey</ly-as-4314610>" itemprop="logo-yshey"></span>
    </div>
    <div id="jesuarva" class="row">    
		<div class="newsletter">
  			<div class="wrapper">
            {% form 'customer' %}

              {{ form.errors | default_errors }}

              {% if form.posted_successfully? %}
                <div class="newsletter--form">
          			<div class="note form-success"><ly-as-4285805>Thank you for subscribing!</ly-as-4285805></div>
        		</div>
              {% else %}
                
                <label for="Email" class="newsletter__label hidden-label">{% comment %}ly_i18n_replace_for_[ 'general.newsletter_form.newsletter_email' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.newsletter_form.newsletter_email' | t }}{% endcapture %}{% include 'ly-static-string' with '4285809' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
        			<input type="hidden" name="contact[tags]" value="newsletter">
        				<div style="background-color: rgba(255,255,255,0.5)" class="newsletter--form">
          
          					<div class="input-group">
                            <input type="hidden" name="contact[tags]" value="newsletter">
            				<input type="email" value="{% if customer %}{{ customer.email }}{% endif %}" placeholder="<ly-as-4285808>Email address...</ly-as-4285808>" name="contact[email]" id="Email" class="input-group-field newsletter__input" autocorrect="off" autocapitalize="off">
            					<span class="input-group-btn">
                                  <button type="submit" class="btn newsletter__submit" name="commit" id="Subscribe">
                                    <span class="newsletter__submit-text--large"><ly-as-4285806>SUBSCRIBE NOW</ly-as-4285806></span>
                                    <span class="newsletter__submit-text--small">
                                      <span class="icon icon-arrow-right" aria-hidden="true"></span>
                                    </span>
                                  </button>
            					</span>
          					</div>
        				</div>
              {% endif %}

            {% endform %}
          </div>
      </div>
    </div>
       


  </div>
  </div>
 <script type="text/javascript">
    function addPrice(price){
      return Shopify.formatMoney(price, "{{ shop.money_format }}").replace('.00', '');
    }
  </script>
  <script type="text/javascript">

  </script>
  


</body>
</html>
