{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}
{% layout none %}
<!DOCTYPE html>
<html>

<head>

  <title><ly-as-4285798>Will be opening soon...</ly-as-4285798></title>
  <style type="text/css">
    * {
      margin: 0;
      padding: 0;
    }

    img {
      border: 0;
    }

    body {
      margin: 0;
      background-color: #e0e0e0;
      font: 1em/1.4em "Lucida Grande", "Trebuchet MS", Helvetica, sans-serif;
      color: #333;
    }

    h1,h2,h3,h4,h5,h6 {
      color: #000;
      margin: 5px 0 10px 0;
      font-weight: 400;
    }

    h3,h4,h5,h6 {
      font-weight: 700;
    }

    a {
      color: #3084CA;
    }

    a:hover {
      color: #25669C;
    }

    p {
      color: #666;
      margin-bottom: 13px;
      font-size: 14px;
    }

    ul, ol {
      margin-bottom: 13px;
      margin-left: 15px;
    }

    ol {
      margin-left: 25px;
    }

    input.btn {
      font-weight: bold;
      cursor: pointer;
      border: 1px solid #bbb;
      border-top: 1px solid #ccc;
      border-bottom: 1px solid #aaa;
      background-color: #ddd;
      background-image: url('data:image/jpeg;base64,');
      background-position: top left;
      background-repeat: repeat-x;
      padding: 4px 6px;
      -moz-border-radius: 5px;
      -webkit-border-radius: 5px;
      border-radius: 5px;
      text-shadow: 0px 1px 1px #fff;
      color: #3a3a3a;
    }

    input.btn:hover {
      -webkit-box-shadow: 0px 0px 3px #3084CA;
      -moz-box-shadow: 0px 0px 3px  #3084CA;
    }

    input.btn:active {
      outline: none;
      -moz-outline-style: none;
      background: #eee;
      border-top: 1px solid #aaa;
      border-bottom: 1px solid #ccc;
      color: #555;
    }

    #container {
      width: 500px;
      margin: 50px auto 0 auto;
    }

    #login {
      background: #fff;
      border: none;
      border-bottom: 1px solid #ccc;
      margin: 50px auto 0 auto;
      padding: 30px;
      width: 450px;
      -webkit-border-radius: 4px;
      -moz-border-radius: 4px;
      border-radius: 4px;
      box-shadow: 0 0 10px rgba(0,0,0,0.1);
      -moz-box-shadow: 0 0 10px rgba(0,0,0,0.1);
      -webkit-box-shadow: 0 0 10px rgba(0,0,0,0.1);
      box-shadow: 0 0 10px rgba(0,0,0,0.1);
    }

    #login h1 {
      margin: 0 0 18px 0;
      text-align: center;
      line-height: 30px;
    }

    #login .default-text {
      font-family: serif;
      font-style: italic;
      margin-bottom: 1.5em;
      font-size: 17px;
      display: block;
      text-align: center;
      margin-top: 33px;
    }

    form#login_form {
      border-top: 1px solid #e0e0e0;
      padding-top: 25px;
      margin-top: 25px;
      font-size: 80%;
      margin-bottom: 35px;
    }

    form#login_form label {
      color: #777;
    }

    #login_form #password {
      font-size: 14px;
      padding: 4px 6px;
      border: 1px solid #ccc;
      outline: none;
      background: #e9e9e9;
      background: -moz-linear-gradient(top, #ececec 0%, #fff 100%);
      background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ececec), color-stop(100%,#fff));
    }

    #login_form label, #login_form input {
      float: left;
      margin-right: 3px;
    }

    #footer {
      margin-top: 15px;
      margin-bottom: 40px;
      font-size: 13px;
      color: #666;
      text-shadow: 0 1px rgba(255,255,255,0.4);
      text-align: center;
    }

    #powered {
      color: #9c9c9c;
      display: block;
      margin-top: 15px;
    }

    #powered a {
      background-repeat: no-repeat;
      background-image: url(data:image/png;base64,);
      padding-left: 21px;
      color: #9c9c9c;
      text-decoration: none;
    }

    #powered a:hover {
      text-decoration: underline;
    }

    .errors {
      background: #feebeb;
      padding: 4px 5px;
      font-size: 15px;
      color: #7e1412;
      display: block;
      margin-bottom: 15px;
    }

    .errors ul {
      list-style: none;
      padding: 0;
      margin: 0;
    }

    .center {
      text-align: center;
      display: block;
      font-size: 17px;
    }
    #passwordHome .showMore {
    display: none;
}
#login .default-text {
    margin-bottom: 1.5em;
    font-size: 17px;
    display: block;
    text-align: center;
    margin-top: 33px;
    font-family: 'AzoSans-Regular';
    text-transform: uppercase;
    font-style: inherit;
}
#login h1 {
    margin: 0 0 18px 0;
    text-align: center;
    line-height: 30px;
    display: none;
}
input.btn {
    border: 0;
    margin-left: 5px;
    font-family: 'AzoSans-Medium';
    color: #FF006C;
    text-transform: uppercase;
    background-color: #FFF;
    font-size: 14px;
    font-weight: 100;
    padding-left: 17px;
    padding-right: 17px;
    padding-top: 7px;
    padding-bottom: 3px;
    letter-spacing: 1.6px;
    border-radius: 2px;
    transition: .4s ease-in-out;
    border: 1px #FF006C solid;
    box-shadow: inherit;
    background: transparent;
}

input.btn:hover {
    color: #FFF;
    background-color: #FF006C;
}
form#login_form label {
    display: none;
}
form#login_form {
  border-top: 1px solid #e0e0e0;
  padding-top: 25px;
  margin-top: 25px;
  font-size: 80%;
  max-width: 291px;
  margin: 0 auto;
  margin-bottom: 35px;
}
span.laun {
    position: absolute;
    top: 25%;
    text-align: center;
    z-index: 1000;
    width: 100%;
    font-size: 27px;
    color: #fff;
    font-family: 'AzoSans-Regular';
}

span.weare {
    position: absolute;
    top: 69%;
    text-align: center;
    z-index: 1000;
    width: 100%;
    font-size: 23px;
    color: #fff;
    font-family: 'AzoSans-Light';
    letter-spacing: 1px;
}
  </style>
</head>
<body id='passwordHome'>
  <span class='laun'><ly-as-4285799>Launching April 2017</ly-as-4285799></span>
  <span class='weare'><ly-as-4285800>We're working on creating something fabulous!</ly-as-4285800></span>
  {% section 'header' %}
  <div id='container'>
    <div id='login'>
      <div>
        <h1>{{ langify_shop_name }}</h1>
        {% if shop.password_message and shop.password_message != '' %}
          {{ shop.password_message }}
        {% else %}
        <p></p>
        {% endif %}
      </div>
      {% form 'storefront_password' %}
        {{ form.errors | default_errors }}
        <label for='password'><ly-as-4285801>Enter store using password:</ly-as-4285801> </label>
        <input type='password' style='background-color=#45B39D;' name='password' id='password'/>
        <div class="actions">
          <input class="btn"  name="commit" type="submit" value="<ly-as-4285802>Enter</ly-as-4285802>" />
        </div>
      {% endform %}
    </div>

    <div id='footer'>
      
    </div>
  </div>
 <script type="text/javascript">
    function addPrice(price){
      return Shopify.formatMoney(price, "{{ shop.money_format }}").replace('.00', '');
    }
  </script>
  <script type="text/javascript">

  </script>
  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js' | script_tag }}
  {% comment %}ly_asset_replace_for_[ 'jquery.fancybox.pack.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'jquery.fancybox.pack.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'jquery.mousewheel.pack.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'jquery.mousewheel.pack.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'shop.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'shop.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'style.css' | asset_url | stylesheet_tag ]_begin{% endcomment %}{% assign ly_asset = 'style.css' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | stylesheet_tag }}{% comment %}ly_asset_replace_end{% endcomment %}


</body>
</html>
